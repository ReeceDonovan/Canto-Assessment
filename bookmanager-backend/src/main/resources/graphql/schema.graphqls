type Query {
    findAllBooks: [Book!]!
    findBookById(id: Int): Book
    findBooksByDate(startDate: String!, endDate: String): [Book!]!
}

type Mutation {
    createBook(title: String!, author: String!, publishedDate: String!): Book!
    deleteBook(id: Int!): Int!
}

type Book {
    id: Int
    title: String
    author: String
    publishedDate: String
}
